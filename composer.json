{
  "name": "whotrades/fb-messenger-sdk",
  "description": "Facebook Messenger Bot php sdk",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Gallice",
      "email": "tm.gallice@gmail.com"
    },
    {
      "name": "Vladimir Karpov",
      "email": "vkarpov@corp.finam.ru"
    }
  ],
  "keywords": [
    "facebook",
    "messenger",
